{
  "manifest_version": 3,
  "name": "Uptime",
  "short_name": "Browser Uptime",
  "version": "0.6",
  "description": "Tracks browser uptime",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "action": {
    "default_icon": "icon_19.png"
  },
  "background": {
    "service_worker": "uptime.js"
  },
  "permissions": ["notifications", "storage"],
  "web_accessible_resources": [
    {
      "resources": ["icon_48.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
